@{
    ViewBag.Title = "CreateVocaCategory";
}

<header id="head" class="secondary">
    <div class="container">
        <div class="row">
            <div class="col-sm-8">
                <h2>
                    TẠO BỘ TỪ VỰNG</h2>
            </div>
        </div>
    </div>
</header>
<br />
<div id="error-message" class="text-danger">@TempData["ErrorMessage"]</div>
<section class="container-fluid">
    <div class="container container-pad" id="property-listings">
        <div class="row">
            Voca category
            <div class="col-md-12">
                @using (Html.BeginForm("CreateVocaCategory", "Account", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    
                    <table>
                        <tr>
                            <td>
                                <input type="file" id="file" name="file" /></td>
                        </tr>
                        <tr>
                            <td>
                                <input type="submit" id="Submit" name="Submit" value="Submit" /></td>
                        </tr>
                    </table>
                }
            </div>

            Kanji example
            <div class="col-md-12">
                @using (Html.BeginForm("CreateKanjiExample", "Account", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    <table>
                        <tr>
                            <td>
                                <input type="file" id="file1" name="file" /></td>
                        </tr>
                        <tr>
                            <td>
                                <input type="submit" id="Submit1" name="Submit" value="Submit" /></td>
                        </tr>
                    </table>
                }
            </div>
        </div>
    </div>
</section>
