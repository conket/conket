                            </div>
                        </div>
                    </li>
                </ul>*@

                <ul class="nav navbar-nav navbar-right user-nav">
                    <li class="user-name"><span>@Session["DisplayName"]</span></li>
                    <li class="dropdown avatar-dropdown">
                        <img src="@Html.Raw(Session["UrlImage"])" class="img-circle avatar" alt="user name" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true" />
